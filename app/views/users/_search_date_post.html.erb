<h6 class="search_window_post">投稿数の検索(日別)</h6>
<%= form_with url: search_post_user_path(user.id), method: :get, class: "form-inline ml-auto", local:false do |f| %>
  <%= f.date_field :search_date, class: "form-group form-control mr-sm-2" %>
  <%= f.submit '検索', name: nil, class: "btn btn-outline-success my-2 my-sm-0" %>
<% end %>
<div id="search_result_post">
</div>